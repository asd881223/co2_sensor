# Main-build Target
main-build: ot-ble-dmp.axf

# Tool invocations
ot-ble-dmp.axf: $(OBJS) $(USER_OBJS) makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU ARM C++ Linker'
	arm-none-eabi-g++ '@ot-ble-dmp.axf.rsp' -o ot-ble-dmp.axf 
	@echo 'Finished building target: $@'
	@echo ' '

	@echo 'Building out file: ot-ble-dmp.out'
	arm-none-eabi-objcopy "ot-ble-dmp.axf" "ot-ble-dmp.out"
	@echo ' '

	@echo 'Building hex file: ot-ble-dmp.hex'
	arm-none-eabi-objcopy -O ihex "ot-ble-dmp.axf" "ot-ble-dmp.hex"
	@echo ' '

	@echo 'Building bin file: ot-ble-dmp.bin'
	arm-none-eabi-objcopy -O binary "ot-ble-dmp.axf" "ot-ble-dmp.bin"
	@echo ' '

	@echo 'Building s37 file: ot-ble-dmp.s37'
	arm-none-eabi-objcopy -O srec "ot-ble-dmp.axf" "ot-ble-dmp.s37"
	@echo ' '

	@echo 'Running size tool'
	arm-none-eabi-size "ot-ble-dmp.axf" -A
	@echo ' '

# Other Targets
clean:
	-@$(foreach FILE, $(CC_DEPS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(C++_DEPS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(EXECUTABLES), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(OBJS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(C_UPPER_DEPS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(CXX_DEPS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(C_DEPS), $(shell $(RM) $(FILE)) )
	-@$(foreach FILE, $(CPP_DEPS), $(shell $(RM) $(FILE)) )
	-$(RM) ot-ble-dmp.axf
	-@echo ' '

